config openlistui 'main'
	option enabled '1'
	option port '5244'
	option data_dir '/etc/openlistui'
	option auto_start '0'
	option enable_https '0'
	option https_port '5443'
	option force_https '0'
	option enable_logging '1'
	option log_file '/var/log/openlistui.log'
	option log_max_size '4'
	option log_cache_hours '48'

config openlistui 'integration'
	option kernel_save_path '/tmp/openlist'